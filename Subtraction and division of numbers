
import java.util.Scanner;

class Codechef
{
	public static void main (String[] args)
	{
		Scanner read = new Scanner(System.in);
		
		int t = read.nextInt();
		for (int i=0; i<t; i++)
		{
    		int a = read.nextInt();
    		int b = read.nextInt();
    		
    		// Difference of inputs
    		int d = a - b;
    		
    		// Division of inputs.
            int q = a / b;
    		
    		// print the desired output for each test case
    		System.out.println(d + " " + q);
		}
	}
}
